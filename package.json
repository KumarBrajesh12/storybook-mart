{
  "name": "storybook-mart",
  "version": "0.1.9",
  "type": "module",
  "main": "dist/index.tsx",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*",
      "require": "./dist/components/*"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.106",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/parser": "^7.25.4",
    "@babel/types": "^7.25.4",
    "@chromatic-com/storybook": "^1.7.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/preset-create-react-app": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-webpack5": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@testing-library/react": "^16.0.0",
    "autoprefixer": "^10.4.20",
    "csstype": "^3.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.41",
    "prop-types": "^15.8.1",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.10",
    "web-vitals": "^4.2.3",
    "webpack": "^5.94.0"
  },
  "description": "storybook-mart",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KumarBrajesh12/storybook-mart.git"
  },
  "keywords": [
    "storybook",
    "mart",
    "UI",
    "frontend"
  ],
  "author": "Brajesh Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KumarBrajesh12/storybook-mart/issues"
  },
  "homepage": "https://github.com/KumarBrajesh12/storybook-mart#readme"
}
